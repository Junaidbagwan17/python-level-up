height = float(input("height: "))
weight = int(input("weight: "))

bmi1 = weight / height ** 2
bmi = round(bmi1,1)
print(bmi)

if bmi <= 18.5 :
    print("underweight")
elif bmi > 18.5 and bmi <=25:
    print("normal weight")
elif bmi >= 25 and bmi <= 30 :
    print("overweight")
elif bmi > 30 and bmi <= 35 :
    print("obese")
else: 
    print("clnically obese")