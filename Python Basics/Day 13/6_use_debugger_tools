# use a debugger addd breakpoints and fix
def mutate(a_list):
    b_list = []
    for item in a_list:
        new_item = item * 2
    b_list.append(new_item)
    print(b_list)
mutate([1,2,3,4,5,6,7])

# use a debugger addd breakpoints and fix
# solution
def mutate(a_list):
    b_list = []
    for item in a_list:
        new_item = item * 2
        b_list.append(new_item)
    print(b_list)
mutate([1,2,3,4,5,6,7])